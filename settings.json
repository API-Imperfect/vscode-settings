{
   "workbench.startupEditor": "newUntitledFile",
   "editor.fontSize": 27,
   "terminal.integrated.fontSize": 16,
   "terminal.integrated.fontFamily": "Jetbrains Mono",
   "editor.minimap.enabled": false,
   "[html]": {
      "editor.defaultFormatter": "vscode.html-language-features"
   },

   "editor.formatOnSave": true,
   "git.enableSmartCommit": true,
   "workbench.iconTheme": "material-icon-theme",
   // "materialTheme.accent": "Acid Lime",
   "workbench.colorCustomizations": {
      "terminal.background": "#000000",
      "sideBar.background": "#1a1919",
      "selection.background": "#C6FF0040",
      "editor.background": "#000000",
      "tree.indentGuidesStroke": "#3cff008f",
      "activityBar.activeBackground": "#1c62ba",
      "activityBar.activeBorder": "#ea74a9",
      "activityBar.background": "#1c62ba",
      "activityBar.foreground": "#e7e7e7",
      "activityBar.inactiveForeground": "#e7e7e799",
      "activityBarBadge.background": "#ea74a9",
      "activityBarBadge.foreground": "#15202b",
      "statusBar.background": "#154b8e",
      "statusBar.foreground": "#e7e7e7",
      "statusBarItem.hoverBackground": "#1c62ba",
      "titleBar.activeBackground": "#154b8e",
      "titleBar.activeForeground": "#e7e7e7",
      "titleBar.inactiveBackground": "#154b8e99",
      "titleBar.inactiveForeground": "#e7e7e799"
   },
   "editor.tokenColorCustomizations": {
      "[Default Dark+]": {
         "comments": "#75715E",
         "textMateRules": [
            {
               "scope": "source.env",
               "settings": {
                  "foreground": "#66D9EF"
               }
            },
            {
               "scope": "variable.other.env",
               "settings": {
                  "foreground": "#fc9867"
               }
            },
            {
               "scope": "constant.numeric.env",
               "settings": {
                  "foreground": "#a6e22e"
               }
            },

            {
               "scope": "punctuation.definition.string",
               "settings": {
                  "foreground": "#e66666"
               }
            },

            {
               "scope": "keyword.control.import.python",
               "settings": {
                  "foreground": "#FF6188"
               }
            },
            {
               "scope": "keyword.control.import.from.python",
               "settings": {
                  "foreground": "#FF6188"
               }
            },
            {
               "scope": "string.quoted.double.single-line.python",
               "settings": {
                  "foreground": "#FFD866"
               }
            },
            {
               "scope": "string.quoted.single.single-line.python",
               "settings": {
                  "foreground": "#FFD866"
               }
            },
            {
               "scope": "punctuation.separator.key-value.ini",
               "settings": {
                  "foreground": "#FF6188"
               }
            },

            {
               "scope": "string.quoted.double.block.python",
               "settings": {
                  "foreground": "#a6e22e"
               }
            },
            {
               "scope": "storage.type.class.python",
               "settings": {
                  "foreground": "#FD971F"
               }
            },
            {
               "scope": " entity.name.tag.yaml",
               "settings": {
                  "foreground": "#FD971F"
               }
            },

            {
               "scope": "punctuation.definition.entity.ini",
               "settings": {
                  "foreground": "#FD971F"
               }
            },
            {
               "scope": "support.function.builtin.shell",
               "settings": {
                  "foreground": "#FD971F"
               }
            },

            {
               "scope": "entity.name.type.class",
               "settings": {
                  "foreground": "#a6e22e"
               }
            },
            {
               "scope": "constant.numeric.integer",
               "settings": {
                  "foreground": "#a6e22e"
               }
            },
            {
               "scope": "support.function.magic.python",
               "settings": {
                  "foreground": "#a6e22e"
               }
            },
            {
               "scope": "constant.numeric",
               "settings": {
                  "foreground": "#a6e22e"
               }
            },
            {
               "scope": "string",
               "settings": {
                  "foreground": "#a6e22e"
               }
            },

            {
               "scope": "entity.name.namespace",
               "settings": {
                  "foreground": "#FCFCFA"
               }
            },
            {
               "scope": "entity.name.function",
               "settings": {
                  "foreground": "#fd971F"
               }
            },
            {
               "scope": "meta.function-call.python",
               "settings": {
                  "foreground": "#fd971F"
               }
            },

            {
               "scope": "variable",
               "settings": {
                  "foreground": "#FCFCFA"
               }
            },
            {
               "scope": "constant.language.python",
               "settings": {
                  "foreground": "#FCFCFA"
               }
            },
            {
               "scope": "storage.type.function.python",
               "settings": {
                  "foreground": "#F92672"
               }
            },
            {
               "scope": " keyword.control",
               "settings": {
                  "foreground": "#F92672"
               }
            },

            {
               "scope": "keyword.operator",
               "settings": {
                  "foreground": "#F92672"
               }
            },
            {
               "scope": "keyword.control.flow.python",
               "settings": {
                  "foreground": "#AE81FF"
               }
            },
            {
               "scope": "entity.other.inherited-class.python",
               "settings": {
                  "foreground": "#fd971F"
               }
            },
            {
               "scope": "keyword.other.definition.ini",
               "settings": {
                  "foreground": "#569CD6"
               }
            },
            {
               "scope": "source.ini",
               "settings": {
                  "foreground": "#FFD866"
               }
            },
            {
               "scope": "entity.name.class",
               "settings": {
                  "foreground": "#FCFCFA"
               }
            }
         ]
      }
   },
   "peacock.color": "#154b8e",
   "editor.wordWrap": "on",
   "liveServer.settings.ChromeDebuggingAttachment": false,
   "liveServer.settings.CustomBrowser": "firefox",
   "[javascript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
   },
   "[python]": {
      "editor.defaultFormatter": null
   },
   "files.associations": {
      "*.js": "javascript"
   },
   "editor.linkedEditing": true,
   "editor.snippetSuggestions": "top",
   "editor.tabCompletion": "on",
   "editor.quickSuggestions": true,
   "editor.codeActionsOnSave": {
      "source.organizeImports": true
   },

   // eslint
   "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact"
   ],
   "workbench.tree.indent": 20,
   "workbench.tree.renderIndentGuides": "onHover",

   "[typescript]": {
      "editor.formatOnPaste": true,
      "editor.formatOnSave": true,
      "editor.codeActionsOnSave": {
         "source.organizeImports": true
      }
   },
   "[typescriptreact]": {
      "editor.codeActionsOnSave": {
         "source.organizeImports": true
      }
   },
   "typescript.preferences.importModuleSpecifier": "relative",
   "typescript.updateImportsOnFileMove.enabled": "always",
   "window.zoomLevel": 1,
   "editor.fontFamily": "Jetbrains Mono",
   "editor.fontWeight": "400",
   "editor.fontLigatures": true,
   "files.exclude": {
      "package-lock.json": true,
      "yarn.lock": true
   },
   "window.title": "${activeEditorMedium}${separator}${rootName}${rootPath}",
   "python.showStartPage": false,
   "editor.formatOnPaste": true,
   "javascript.updateImportsOnFileMove.enabled": "always",
   "editor.defaultFormatter": "esbenp.prettier-vscode",
   "[json]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
   },
   "json.format.enable": false,
   "hediet.vscode-drawio.local-storage": "",
   "prettier.tabWidth": 3,
   "editor.multiCursorModifier": "alt",
   "explorer.confirmDelete": false,
   "editor.suggestSelection": "first",
   "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
   "python.languageServer": "Pylance",
   "kite.showWelcomeNotificationOnStartup": false,
   "emmet.includeLanguages": {
      "javascript": "javascriptreact"
   },
   "javascript.preferences.importModuleSpecifier": "relative",
   "workbench.editorAssociations": [
      {
         "viewType": "jupyter.notebook.ipynb",
         "filenamePattern": "*.ipynb"
      }
   ],
   "kite.codefinder.enableLineDecoration": false,
   "explorer.compactFolders": false,
   "python.formatting.provider": "black",
   "files.autoSaveDelay": 2000,
   "debug.console.fontFamily": "Jetbrains Mono",
   "explorer.confirmDragAndDrop": false
   // "http.proxy": "off"
}
